<meta property="og:site_name" content="{{appName}}"/>
<meta property="og:title" content="{{#if htmltitle}}{{htmltitle}}{{else}}{{appName}}{{/if}}" />
<meta property="og:description" content="{{#if appOGDescription}}{{appOGDescription}}{{else}}Powered by NYU Digital Library Technology Services{{/if}}"/>
<meta property="og:type" content="website" />
<meta property="og:url" content="{{#if appOGUrl}}{{appOGUrl}}{{else}}{{appUrl}}{{/if}}" /> 
{{#appOGImage}}<meta property="og:image" content="{{path}}" >{{/appOGImage}}
