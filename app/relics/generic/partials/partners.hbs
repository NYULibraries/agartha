<!doctype html>
<html class="nojs">
<head>{{> head }}</head>
<body data-app="{{ appUrl }}" data-appRoot="{{ appRoot }}" class="{{ bodyClass }}" data-script='{ "js" : [ "commons.js" ] }'>
  {{> header }}
  <div class="container-fluid"><div class="flex-container">
    <main class="main " role="main">
    {{#title}}
      <h2  class="page-title">{{{html}}}</h2>
    {{/title}}
    {{#content}}
      {{#main}}
        <div class="maintext {{class}}" >{{{html}}}</div>
      {{/main}}
    {{/content}}
    </main>
 </div></div>
  {{> footer }}
  {{{ closure }}}  
</body>
</html>
